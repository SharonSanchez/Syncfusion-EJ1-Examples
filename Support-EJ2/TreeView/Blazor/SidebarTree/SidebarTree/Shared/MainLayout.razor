{
        {"class", "sidebar-treeview" }
    };
    public void Toggle()
    {
        sidebar.Toggle();
    }

    // Triggers when expanding a node
    async void OnExpand(NodeExpandEventArgs args)
    {
        if (args.IsInteracted)
        {
            if (args.NodeData.ParentID != null)
            {
                List<TreeData> result = NavigationItems.FindAll(f => f.Pid == args.NodeData.ParentID);
                foreach (TreeData x in result)
                {
                    if (x.Id != args.NodeData.Id)
                    {
                        Arr.Add(x.Id);
                    }

                }
                string[] Node = Arr.ConvertAll(x => x.ToString()).ToArray();
                // To collapse remaining nodes
                treeView.CollapseAll(Node);
            }
            else
            {
                // To collapse the other root level nodes
                treeView.CollapseAll();
            }
        }
        Arr.Clear();
    }

