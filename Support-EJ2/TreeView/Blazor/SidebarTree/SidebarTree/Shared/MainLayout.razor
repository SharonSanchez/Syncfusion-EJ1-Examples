@inherits LayoutComponentBase

@using SidebarTree.Data
@inject WeatherForecastService ForecastService
@using Syncfusion.EJ2.Blazor.Navigations
@using Syncfusion.EJ2.Blazor.Inputs
@using System.Threading.Tasks;
<div class="main">
    <div id="wrapper">
        <div class="col-lg-12 col-sm-12 col-md-12">
            <!--header-section  declaration -->
            <div class="main-header" id="header-section">
                <ul class="header-list">
                    <li class="float-left header-style oi oi-menu" id="hamburger" @onclick="@Toggle"></li>
                    <li class="float-left header-style nav-pane"><b>SideBar TreeView</b></li>
                    <li class="header-style float-right support border-left"><b>Login</b></li>
                </ul>
            </div>
            <!-- end of header-section -->
            <!-- sidebar element declaration -->
            <EjsSidebar Animate="false" EnableGestures="false" HtmlAttributes="@htmlAttribute" Width="290px" @ref="sidebar" MediaQuery="min-width:1400px">
                <ChildContent>
                    <div class="main-menu">
                        <button @onclick="@click">Click me to load Tree</button>
                        <div>
                            @* LicenseView MENU *@
                            <p class="text-left sidebar-fav">Main</p>
                            <hr style="margin:0;" />
                            <TreeMenu></TreeMenu>
                            @* System MENU *@
                            <p class="text-left sidebar-fav">System</p>
                            <hr style="margin:0;" />
                            <CustomMenu></CustomMenu>
                            <p class="text-left sidebar-fav">License Admin</p>
                            <EjsTreeView ModelType="@ModelType" @ref="treeView" TValue="TreeData" ExpandOn="ExpandOnSettings.Click" CssClass="main-treeview">
                                <TreeViewEvents TValue="TreeData" NodeExpanding="OnExpand"></TreeViewEvents>
                                <TreeViewFieldsSettings Id="Id"
                                                        Text="Name"
                                                        ParentID="Pid"
                                                        HasChildren="HasChild"
                                                        IconCss="Icon"
                                                        Selected="Selected"
                                                        NavigateUrl="Url"
                                                        Expanded="Expanded"
                                                        DataSource="@NavigationItems"></TreeViewFieldsSettings>
                            </EjsTreeView>

                        </div>
                    </div>
                </ChildContent>
            </EjsSidebar>
            <!-- end of sidebar element -->
            <!-- main-content declaration -->
            <div class="main-content" id="main-text">
                <div class="sidebar-content">
                    @Body
                </div>
            </div>
        </div>
    </div>
</div>
@code {
