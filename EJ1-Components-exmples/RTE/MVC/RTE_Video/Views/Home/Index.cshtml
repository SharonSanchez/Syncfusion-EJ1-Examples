@{
    /**/

    ViewBag.Title = "Home Page";
}

@{
    List<String> toolsList = new List<string>() { "links", "images", "media" };
    List<String> links = new List<string>() { "createLink" };
    List<String> images = new List<string>() { "image" };
    List<String> media = new List<string>() { "video" };
}
@Html.EJ().RTE("rteSample").Width("820px").Height("400px").ContentTemplate(@<iframe width="560" height="315" src="https://www.youtube.com/embed/cQZYzOlTm0Q" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>).ToolsList(toolsList).Tools(tool => tool.Media(media).Links(links).Images(images))